    #java -jar ~/phylogenetic_tools/astral.4.7.6.jar -i ~/scratch/AGBsvdquartets/data/${PBS_ARRAYID}/all_RAxML_${N}.trees -o ~/scratch/AGBsvdquartets/data/${PBS_ARRAYID}/new_astral_s_tree_${N}.trees
done

K=1000
